package appleclient.mods;

import appleclient.Apple;

public class Mod
{
    private boolean enabled;
    public String name;
    
    public Mod(String name)
    {
        this.name = name;
    }
    
    public void onEnable()
    {
        enabled = true;
        Apple.CLIENT.eventBus.register(this);
    }
}
